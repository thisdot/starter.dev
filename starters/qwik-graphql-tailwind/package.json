{
  "name": "qwik-graphql-tailwind",
  "description": "Qwik, GraphQL, and TailwindCSS",
  "keywords": [
    "qwik",
    "graphql",
    "tailwind"
  ],
  "version": "0.0.1",
  "hasShowcase": false,
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint \"src/**/*.ts*\"",
    "prettier": "prettier --write \"**/*.+(ts|tsx)\"",
    "prettier.check": "prettier --check \"**/*.+(ts|tsx)\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open http://localhost:5173 --mode ssr",
    "qwik": "qwik",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "test.coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.11.1",
    "@builder.io/qwik-city": "0.0.112",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/html": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@types/eslint": "8.4.7",
    "@types/node": "18.11.3",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "@vitest/coverage-c8": "^0.24.3",
    "autoprefixer": "10.4.12",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qwik": "0.11.1",
    "eslint-plugin-storybook": "0.6.6",
    "node-fetch": "3.2.10",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "tailwindcss": "3.2.0",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "^0.24.3"
  }
}
