{
  "name": "lit-mobx-vaadin",
  "description": "Lit, Mobx, and Vaadin",
  "keywords": [
    "lit",
    "mobx",
    "vaadin"
  ],
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "prettier": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "storybook": "tsc && npm run analyze -- --exclude dist && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
    "storybook:build": "tsc && npm run analyze -- --exclude dist && build-storybook",
    "test": "tsc && wtr --coverage",
    "test:integration": "playwright test",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && npm run analyze -- --exclude dist",
    "start:build": "web-dev-server --root-dir dist --app-index index.html --open",
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "dev": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\""
  },
  "dependencies": {
    "@adobe/lit-mobx": "2.2.0",
    "@vaadin/app-layout": "24.0.5",
    "@vaadin/button": "24.0.5",
    "@vaadin/horizontal-layout": "24.0.5",
    "@vaadin/router": "1.7.5",
    "@vaadin/vaadin-themable-mixin": "24.0.5",
    "@vaadin/vertical-layout": "24.0.5",
    "lit": "2.7.4",
    "mobx": "6.9.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.21.5",
    "@custom-elements-manifest/analyzer": "0.8.2",
    "@open-wc/eslint-config": "10.0.0",
    "@open-wc/testing": "3.1.8",
    "@playwright/test": "1.33.0",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-replace": "5.0.2",
    "@types/mocha": "10.0.1",
    "@types/node": "20.1.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@web/dev-server": "0.2.1",
    "@web/dev-server-rollup": "0.5.0",
    "@web/dev-server-storybook": "0.7.1",
    "@web/rollup-plugin-html": "2.0.0",
    "@web/rollup-plugin-import-meta-assets": "2.0.0",
    "@web/storybook-prebuilt": "0.1.34",
    "@web/test-runner": "0.16.1",
    "@web/test-runner-playwright": "0.10.0",
    "babel-plugin-template-html-minifier": "4.1.0",
    "concurrently": "8.0.1",
    "deepmerge": "4.3.1",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "rollup": "3.21.7",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-workbox": "7.0.1",
    "storybook": "7.0.11",
    "tslib": "2.5.0",
    "typescript": "5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "customElements": "custom-elements.json"
}
