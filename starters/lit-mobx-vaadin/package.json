{
  "name": "starter-app",
  "description": "Webcomponent starter-app following open-wc recommendations",
  "license": "MIT",
  "author": "starter-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "storybook": "tsc && npm run analyze -- --exclude dist && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
    "storybook:build": "tsc && npm run analyze -- --exclude dist && build-storybook",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js && npm run analyze -- --exclude dist",
    "start:build": "web-dev-server --root-dir dist --app-index index.html --open",
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\""
  },
  "dependencies": {
    "@adobe/lit-mobx": "^2.2.0",
    "@vaadin/app-layout": "^24.0.3",
    "@vaadin/button": "^24.0.3",
    "@vaadin/horizontal-layout": "^24.0.3",
    "@vaadin/router": "^1.7.5",
    "@vaadin/vaadin-themable-mixin": "^24.0.3",
    "@vaadin/vertical-layout": "^24.0.3",
    "lit": "^2.0.2",
    "mobx": "^6.9.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@open-wc/building-rollup": "^2.0.2",
    "@open-wc/eslint-config": "^9.2.1",
    "@playwright/test": "^1.33.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@web/dev-server": "^0.2.1",
    "@web/dev-server-rollup": "^0.5.0",
    "@web/dev-server-storybook": "^0.7.1",
    "@web/rollup-plugin-html": "^1.11.0",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "@web/storybook-prebuilt": "^0.1.34",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-workbox": "^6.2.0",
    "storybook": "^7.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "customElements": "custom-elements.json"
}
