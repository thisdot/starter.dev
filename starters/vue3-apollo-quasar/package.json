    "graphql": "16.3.0",
    "graphql-tag": "2.12.6",
    "quasar": "2.6.0",
    "vue": "3.2.37",
    "vue-router": "4.0.16"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@quasar/app-vite": "1.0.2",
    "@storybook/addon-actions": "6.4.20",
    "@storybook/addon-essentials": "6.4.20",
    "@storybook/addon-links": "6.4.20",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/vue3": "6.4.20",
    "@types/jest": "27.4.1",
    "@types/node": "12.20.21",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vue/apollo-composable": "4.0.0-alpha.17",
    "@vue/cli-plugin-unit-jest": "5.0.0",
    "@vue/test-utils": "2.0.0",
    "@vue/vue3-jest": "27.0.0",
    "autoprefixer": "9.0.0",
    "babel-jest": "27.0.6",
    "babel-loader": "8.2.4",
    "css-loader": "4.3.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-vue": "8.5.0",
    "husky": "8.0.1",
    "jest": "27.0.5",
    "prettier": "2.5.1",
    "lint-staged": "13.0.3",
    "sass": "1.32.12",
    "sass-loader": "10.0.2",
    "style-loader": "1.3.0",
    "ts-jest": "27.1.4",
    "typescript": "4.5.4",
    "vue-loader": "16.8.3"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "keywords": [
    "vue",
    "apollo",
    "quasar"
  ],
  "productName": "vue3-apollo-quasar starter kit",
  "lint-staged": {
    "**/*.{vue,js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
