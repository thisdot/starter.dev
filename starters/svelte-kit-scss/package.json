{
  "name": "svelte-kit-scss",
  "description": "Create SvelteKit app with SCSS",
  "keywords": [
    "svelte-kit",
    "sass"
  ],
  "private": true,
  "version": "0.1.0",
  "hasShowcase": false,
  "scripts": {
    "start": "vite dev --open",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@fontsource/fira-mono": "4.5.10",
    "@neoconfetti/svelte": "1.0.0",
    "@playwright/test": "1.30.0",
    "@storybook/addon-essentials": "7.0.0-beta.47",
    "@storybook/addon-interactions": "7.0.0-beta.47",
    "@storybook/addon-links": "7.0.0-beta.47",
    "@storybook/blocks": "7.0.0-beta.47",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/svelte": "7.0.0-beta.47",
    "@storybook/sveltekit": "7.0.0-beta.47",
    "@storybook/testing-library": "0.0.13",
    "@sveltejs/adapter-auto": "1.0.3",
    "@sveltejs/kit": "1.5.6",
    "@sveltejs/package": "1.0.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/svelte": "3.2.2",
    "@types/cookie": "0.5.1",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitest/coverage-c8": "0.28.5",
    "babel-loader": "9.1.2",
    "css-loader": "6.7.3",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-storybook": "0.6.10",
    "eslint-plugin-svelte3": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "jsdom": "20.0.3",
    "prettier": "2.8.4",
    "prettier-plugin-svelte": "2.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.58.0",
    "sass-loader": "13.2.0",
    "storybook": "7.0.0-beta.47",
    "style-loader": "3.3.1",
    "svelte": "3.55.1",
    "svelte-check": "2.10.3",
    "svelte-loader": "3.1.5",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.5"
  },
  "type": "module"
}
