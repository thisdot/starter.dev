{
  "name": "remix-gql-tailwind",
  "version": "0.1.0",
  "description": "Remix, GQL and TailwindCSS",
  "keywords": [
    "remix",
    "graphql",
    "tailwind"
  ],
  "hasShowcase": false,
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss ./styles/**/*.css styles --dir ./app/styles",
    "dev": "concurrently -n css,remix -c magenta,cyan \"npm run dev:css\" \"remix dev\"",
    "dev:css": "npm run build:css -- --w",
    "format": "prettier --write ./app",
    "start": "remix-serve build",
    "test": "jest",
    "storybook": "concurrently -n css,storybook -c cyan,magenta \"npm run dev:css\" \"start-storybook -p 6006\"",
    "build-storybook": "build-storybook"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "cross-env": "7.0.3",
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@mdx-js/preact": "2.3.0",
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/react": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "eslint": "8.34.0",
    "eslint-plugin-storybook": "0.6.10",
    "eslint-plugin-unused-imports": "2.0.0",
    "jest": "29.4.2",
    "jest-environment-jsdom": "^29.3.0",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.4",
    "storybook-builder-vite": "0.1.18",
    "tailwindcss": "3.2.6",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5",
    "vite": "3.2.5"
  }
}
